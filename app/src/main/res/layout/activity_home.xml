<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home"
    android:background="#f2f3ff">

    <FrameLayout
        android:id="@+id/homeHead_Frame"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:background="#f6feff"
        >

        <ImageButton
            android:id="@+id/delete_Btn"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:backgroundTint="@android:color/transparent"
            android:scaleType="centerInside"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.038"
            app:srcCompat="@android:drawable/ic_dialog_dialer" />

        <Button
            android:id="@+id/homePhoto_Btn"
            android:layout_width="100dp"
            android:layout_height="50dp"
            android:layout_gravity="center"
            android:layout_marginRight="50dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:text="사진"
            android:padding="10dp"
            />

        <Button
            android:id="@+id/homeMap_Btn"
            android:layout_width="100dp"
            android:layout_height="50dp"
            android:layout_gravity="center"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:text="지도"
            android:padding="10dp"
            />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/homeBody_Frame"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/homeHead_Frame"
        app:layout_constraintBottom_toTopOf="@+id/homeFoot_Frame"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        >

    </FrameLayout>

    <LinearLayout
        android:id="@+id/homeFoot_Frame"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:background="#f6feff"
        >


        <ImageButton
            android:id="@+id/home_Btn"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:backgroundTint="#f6feff"
            android:stateListAnimator="@null"
            android:src="@drawable/home"
            android:adjustViewBounds="true"
            android:scaleType="fitCenter"
            android:visibility="gone"
            android:padding="18dp"
            />

        <ImageButton
            android:id="@+id/homefull_Btn"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:backgroundTint="#f6feff"
            android:stateListAnimator="@null"
            android:src="@drawable/homefull"
            android:adjustViewBounds="true"
            android:scaleType="fitCenter"
            android:visibility="visible"
            android:padding="18dp"
            />

        <ImageButton
            android:id="@+id/heart_Btn"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:backgroundTint="#f6feff"
            android:stateListAnimator="@null"
            android:src="@drawable/heart"
            android:adjustViewBounds="true"
            android:scaleType="fitCenter"
            android:visibility="visible"
            android:padding="18dp"
            />

        <ImageButton
            android:id="@+id/heartfull_Btn"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:backgroundTint="#f6feff"
            android:stateListAnimator="@null"
            android:src="@drawable/heartfull"
            android:adjustViewBounds="true"
            android:scaleType="fitCenter"
            android:visibility="gone"
            android:padding="18dp"
            />

        <ImageButton
            android:id="@+id/add_Btn"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:backgroundTint="#f6feff"
            android:stateListAnimator="@null"
            android:src="@drawable/sum"
            android:adjustViewBounds="true"
            android:scaleType="fitCenter"
            android:visibility="visible"
            android:padding="18dp"
            />

        <ImageButton
            android:id="@+id/addfull_Btn"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:backgroundTint="#f6feff"
            android:stateListAnimator="@null"
            android:src="@drawable/sumfull"
            android:adjustViewBounds="true"
            android:scaleType="fitCenter"
            android:visibility="gone"
            android:padding="18dp"
            />

        <ImageButton
            android:id="@+id/myProfile_Btn"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:backgroundTint="#f6feff"
            android:stateListAnimator="@null"
            android:src="@drawable/user"
            android:adjustViewBounds="true"
            android:scaleType="fitCenter"
            android:visibility="visible"
            android:padding="18dp"
            />

        <ImageButton
            android:id="@+id/myProfilefull_Btn"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:backgroundTint="#f6feff"
            android:stateListAnimator="@null"
            android:src="@drawable/userfull"
            android:adjustViewBounds="true"
            android:scaleType="fitCenter"
            android:visibility="gone"
            android:padding="18dp"
            />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>